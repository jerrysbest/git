            <xml-property name="queryText"><![CDATA[SELECT T6.slpname as 销售员1,T2.U_djNo as 汇总单号,convert(nvarchar(10),T3.DocDate,23) as 日期,T0.Line_ID+1 AS 序号,T3.[CardCode] AS 伙伴代码,T4.[CardName] AS 伙伴名称,T0.Account AS 科目编号,
T1.AcctName AS 科目名称, T0.Debit AS 金额  FROM JDT1 T0  
INNER JOIN OACT T1 ON T1.AcctCode=T0.Account  
INNER JOIN RCT2 T2 ON T2.DocNum=left(T0.LineMemo, charindex('&amp;',T0.LineMemo) - 1)   
INNER JOIN ORCT T3 ON T3.DocNum=T2.DocNum   
INNER JOIN OCRD T4 ON T4.CardCode=T3.CardCode  
INNER JOIN OINV T5 ON T5.docentry=T2.docentry  
INNER JOIN OSLP T6 ON T5.U_slpcode1=T6.slpcode 
where charindex('&amp;',T0.LineMemo)<>0 AND T0.Debit<>0  AND T0.Account<>? AND  T0.Account=? 
