            <xml-property name="queryText"><![CDATA[  select a.orderNo,a.lineNum,a.cinvcode,a.quantity,
	a.orderdate,a.deldate,a.rquantity,state=case when a.quantity=a.rquantity then '全部'
    when a.quantity>a.rquantity then '部分'
    else '未到货'end,
    isnull(a.quantity,0)-isnull(a.rquantity,0) as GAP,a.Memo, sum(b.quantity) AS total到货,count(b.lineNum)as 发货次数,min(b.shipdate)as 首次发货时间,
    OTD= isnull(c.quantity,0)/a.quantity
	 from dbo.pd_porder a 
	 inner join  dbo.pd_GoodReceive b on a.orderNo=b.orderNo and a.linenum=b.linenum
	 left join  dbo.pd_GoodReceive c on a.orderNo=c.orderNo and a.linenum=b.linenum 
	 and c.shipdate<=a.deldate
	 group by a.lineNum,a.orderNo,a.lineNum,a.cinvcode,
	 a.quantity,a.orderdate,a.deldate,a.rquantity,a.Memo,c.quantity
]]></xml-property>
