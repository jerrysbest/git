            <xml-property name="queryText"><![CDATA[select  comcode,cinvcode,cinvname,maxmsale,minmsale,sum(当月销量) as total1,
sortkey=(case when sum(当月销量)-maxmsale>0 then sum(当月销量)-maxmsale 
when sum(当月销量)-minmsale<0 then sum(当月销量)-minmsale else -999999 end) from 
(
(select  comcode='015',cinvcode=replace(a.cinvcode,'_',' '),a.cinvname,a.maxmsale,a.minmsale,
 当月销量=sum(isnull(d.iQuantity,0))
  from bpmapp.dbo.pd_inventory a 
  left join
--发货单
 (select  cinvcode=replace(b.cinvcode,'_',' '),b.iQuantity from erp1.ufdata_015_2012.dbo.dispatchLists b,erp1.ufdata_015_2012.dbo.dispatchlist c 
 where b.dlid=c.dlid and convert(varchar(7),c.ddate,121)=convert(varchar(7),GETDATE(),121)
 )  d 
 on a.cinvcode=d.cInvCode 
 group by a.cinvcode,a.cinvname,a.maxmsale,a.minmsale )
 union
 (select  comcode='015',cinvcode=replace(a.cinvcode,'_',' '),a.cinvname,a.maxmsale,a.minmsale,
 当月销量=sum(isnull(g.iQuantity,0)) 
  from bpmapp.dbo.pd_inventory a   

 --其他出库单
  left join(

 select  cinvcode=replace(b.cinvcode,'_',' '),b.iQuantity  from  erp1.ufdata_015_2012.dbo.RdRecord09 a
	inner join erp1.ufdata_015_2012.dbo.RdRecords09 b on a.id=b.id 
	inner join erp1.ufdata_015_2012.dbo.inventory c on b.cinvcode=c.cinvcode 
	inner join erp1.ufdata_015_2012.dbo.Department d on a.cdepcode=d.cdepcode 
	inner join erp1.ufdata_015_2012.dbo.customer e on a.ccuscode=e.ccuscode 
	inner join erp1.ufdata_015_2012.dbo.CustomerClass f on left(e.ccccode,len(e.ccccode)-2)=f.ccccode 
	inner join erp1.ufdata_015_2012.dbo.warehouse g on a.cwhcode=g.cwhcode 
    inner join erp1.ufdata_015_2012.dbo.Rd_Style h on a.crdcode=h.crdcode 
	where 
	len(e.ccccode)>2 and (h.crdname<>'调拨出库'  or ((h.crdname='其他出库' or h.crdname='其他') and 
	a.ccuscode not in ('CN-ZHJ0571-OMHZ-00','CN-ZHJ0571-OMHZ-01','CN-BEJ0010-OMCN-00','SG-XJP0065-OMSG-00','CN-BEJ0010-OMBJ-00') 
	and a.ccuscode is not null and a.ccuscode<>'' )) 
	and convert(varchar(7),a.ddate,121)=convert(varchar(7),GETDATE(),121)
		)  g  on a.cinvcode=g.cInvCode 
 group by a.cinvcode,a.cinvname,a.maxmsale,a.minmsale  )

 ) kk  group by comcode, cinvcode,cinvname,maxmsale,minmsale 

 union 

 select  comcode,cinvcode,cinvname,maxmsale,minmsale,sum(当月销量) total1,
sortkey=(case when sum(当月销量)-maxmsale>0 then sum(当月销量)-maxmsale 
when sum(当月销量)-minmsale<0 then sum(当月销量)-minmsale else -999999 end) from 
(
(select  comcode='030',cinvcode=replace(a.cinvcode,'_',' '),a.cinvname,a.maxmsale,a.minmsale,
 当月销量=sum(isnull(d.iQuantity,0))
  from bpmapp.dbo.pd_inventory a 
  left join
--发货单
 (select  cinvcode=replace(b.cinvcode,'_',' '),b.iQuantity from erp1.ufdata_030_2013.dbo.dispatchLists b,erp1.ufdata_030_2013.dbo.dispatchlist c 
 where b.dlid=c.dlid and convert(varchar(7),c.ddate,121)=convert(varchar(7),GETDATE(),121)
 )  d 
 on a.cinvcode=d.cInvCode 
 group by a.cinvcode,a.cinvname,a.maxmsale,a.minmsale )
 union
 (select  comcode='030',cinvcode=replace(a.cinvcode,'_',' '),a.cinvname,a.maxmsale,a.minmsale,
 当月销量=sum(isnull(g.iQuantity,0)) 
  from bpmapp.dbo.pd_inventory a   

 --其他出库单
  left join(

 select  cinvcode=replace(b.cinvcode,'_',' '),b.iQuantity  from  erp1.ufdata_030_2013.dbo.RdRecord09 a
	inner join erp1.ufdata_030_2013.dbo.RdRecords09 b on a.id=b.id 
	inner join erp1.ufdata_030_2013.dbo.inventory c on b.cinvcode=c.cinvcode 
	inner join erp1.ufdata_030_2013.dbo.Department d on a.cdepcode=d.cdepcode 
	inner join erp1.ufdata_030_2013.dbo.customer e on a.ccuscode=e.ccuscode 
	inner join erp1.ufdata_030_2013.dbo.CustomerClass f on left(e.ccccode,len(e.ccccode)-2)=f.ccccode 
	inner join erp1.ufdata_030_2013.dbo.warehouse g on a.cwhcode=g.cwhcode 
    inner join erp1.ufdata_030_2013.dbo.Rd_Style h on a.crdcode=h.crdcode 
	where 
	len(e.ccccode)>2 and (h.crdname<>'调拨出库'  or ((h.crdname='其他出库' or h.crdname='其他') and 
	a.ccuscode not in ('CN-ZHJ0571-OMHZ-00','CN-ZHJ0571-OMHZ-01','CN-BEJ0010-OMCN-00','SG-XJP0065-OMSG-00','CN-BEJ0010-OMBJ-00') 
	and a.ccuscode is not null and a.ccuscode<>'' )) 
	and convert(varchar(7),a.ddate,121)=convert(varchar(7),GETDATE(),121)
		)  g  on a.cinvcode=g.cInvCode 
 group by a.cinvcode,a.cinvname,a.maxmsale,a.minmsale  )

 ) kk  group by comcode, cinvcode,cinvname,maxmsale,minmsale order by total1 desc

]]></xml-property>
