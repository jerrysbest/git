            <xml-property name="queryText"><![CDATA[select 流程编号=WF_DocNumber,提交人=a.WF_AddName_CN,部门=c.foldername,申请日期=convert(nvarchar(10),a.WF_DocCreated,23),流程阶段=WF_CurrentNodeName,
变更风险评估=a.xmldata.value('(/Items/WFItem)[1]','nvarchar(20)'),
操作类型=case when a.xmldata.value('(/Items/WFItem/@name)[3]', 'nvarchar(max)')='edit_type' then case when  a.xmldata.value('(/Items/WFItem)[3]', 'nvarchar(max)') =1 then '新增' else '变更' end 
            when a.xmldata.value('(/Items/WFItem/@name)[4]', 'nvarchar(max)')='edit_type' then case when  a.xmldata.value('(/Items/WFItem)[4]', 'nvarchar(max)') =1 then '新增' else '变更' end 
			else '' end ,
公司代码=substring(b.comcode,8,3),物料编码=b.cinvcode,物料名称=b.cinvname from BPM_AllDocument a 
inner join BInventory_Process b on a.WF_OrUnid=b.DocUnid 
inner join BPM_OrgDeptList c on c.deptid=a.WF_AddDeptid 
where a.subject like '%存货档案%' and WF_CurrentNodeName<>'已结束' ]]></xml-property>
